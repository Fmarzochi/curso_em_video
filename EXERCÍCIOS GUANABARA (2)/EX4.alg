algoritmo "escola"
var
 N1, N2, M: Real
inicio
Escreva ("DIGITE A PRIMEIRA NOTA DO ALUNO: ")
   Leia (N1)
    Escreva ("DIGITE A SEGUNDA NOTA DO ALUNO: ")
     Leia (N2)
     M <- (N1 + N2) /2
    Escreval ("O MÉDIA O DA SOMA ENTRE ", N1, " E ", N2, " E IGUAL A ", M)
Escreva ("TENHA UM ÓTIMO DIA ")
fimalgoritmo
